{
    "code": "cacaotablet",

    "variantgroups": [
        //  Whether the Dutch process was performed.
        { "code": "process", "states": [ "natural" ] }, // TODO: add "alkalized" and "ultra-alkalized".
        //  Basic chocolate types.
        { "code": "type", "states": [ "pure" ] }, // TODO: add "dark", "milk", "white", "compounddark", and "compoundmilk"
        //  How finely the chocolate was ground/mixed/conched, and whether it was tempered.
        { "code": "consistency", "states": [ "grainy" ] }, // TODO: add "smooth" and "tempered".
    ],
    "skipVariants": [ // TODO: Prevent "compound" and "tempered" combos

    ],

    "shape": { "base": "item/food/cacaotablet" },
    "textures":{ 
        "cacao": { "base": "item/food/cacaotablet/{process}-{type}-{consistency}"},
    },

    // TODO: Actually fully define this at some point
    "behaviors": [
        {
            "name": "GroundStorable",
            "properties": {
                "layout": "Stacking",
                "collisionBox": { "x1": 0, "y1": 0, "z1": 0, "x2": 1, "y2": 0.125, "z2": 1 },
            }
        }
    ],

    "attributes": {
        // "inContainerTexture": { "base": "item/food/cacaochunks/cacao" },
        "foodTags": [ "chocolate", "{type}-chocolate" ],
        "handbook": {
			"groupBy": ["cacaotablet-*"]
		},
        "nutritionPropsWhenInMealByType": {
            "*": { "satiety": 240, "foodcategory": "Protein" }
        },
        "eatHealthEffectDurationSec": 10,
		"eatHealthEffectTicks": 10,
        "inPiePropertiesByType": {
            "*": { "texture": "block/food/cacao/fill-{process}-{type}-{consistency}", "partType": "Filling", "allowMixing": false }
        }
