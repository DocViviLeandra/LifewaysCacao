    "materialDensity": 1300,

    "nutritionPropsByType": {
        "*": { "satiety": 240, "foodcategory": "Protein" },
    },

    "transitionablePropsByType": {
        "*": [{
            "type": "Perish",
	        "freshHours": { "avg": 2160 },
            "transitionhours": { "avg": 224 },
            "transitionedStack": { "type": "item", "code": "game:rot" },
            "transitionRatio": 0.25
        }]
